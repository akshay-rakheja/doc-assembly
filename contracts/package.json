{
  "name": "@polydocs/contracts",
  "version": "0.1.9",
  "peerDependencies": {
    "@openzeppelin/contracts": "*"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.6.4",
    "@ethersproject/providers": "^5.6.8",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.2",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.3",
    "@nomicfoundation/hardhat-toolbox": "^1.0.2",
    "@nomiclabs/hardhat-ethers": "^2.1.0",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@openzeppelin/contracts": "^4.7.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/mustache": "^4.1.3",
    "chai": "^4.3.6",
    "dotenv": "^16.0.1",
    "ethers": "^5.6.9",
    "hardhat": "^2.10.1",
    "hardhat-gas-reporter": "^1.0.8",
    "hardhat-tracer": "^1.1.0-rc.6",
    "solidity-coverage": "^0.7.21",
    "solidity-docgen": "^0.6.0-beta.22",
    "ts-node": "^10.9.0",
    "typechain": "^8.1.0",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "build:types": "yarn build:types:viewer && yarn build:types:composer && yarn build:types:viewer-gasless && yarn build:types:api",
    "build:types:viewer": "yarn typechain artifacts/contracts/termsable/TermsableNoToken.sol/TermsableNoToken.json --target ethers-v5 --out-dir ../viewer-app/src/contracts",
    "build:types:viewer-gasless": "yarn typechain artifacts/contracts/termsable/TermsableNoToken.sol/TermsableNoToken.json --target ethers-v5 --out-dir ../viewer-gasless/src/contracts",
    "build:types:api": "yarn hardhat typechain && rm -rf ../api/contracts ; cp -r typechain-types ../api/contracts",
    "build:types:composer": "yarn typechain    artifacts/contracts/TermReader.sol/TermReader.json     artifacts/contracts/TermReader.sol/TokenTermReader.json    artifacts/contracts/Termsable.sol/TermsableNoToken.json    artifacts/contracts/Termsable.sol/TokenTermsable.json    --target ethers-v5 --out-dir ../composer-app/src/contracts",
    "docs": "yarn hardhat docgen && cat TOP.md docs/index.md > README.md"
  },
  "files": [
    "contracts/**/*.sol"
  ],
  "license": "MIT",
  "contributors": [
    "Ray Deck",
    "Akshay Rakheja"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/statechangelabs/polydocs.git"
  },
  "homepage": "https://github.com/statechangelabs/polydocs",
  "bugs": {
    "url": "https://github.com/statechangelabs/polydocs/issues"
  },
  "keywords": [
    "solidity",
    "ethereum",
    "smart",
    "contracts"
  ],
  "dependencies": {
    "@types/mocha": "^9.1.1"
  }
}
